{
  "name": "graasp-plugin-thumbnails",
  "version": "0.1.0",
  "description": "Thumbnails fastify plugin for Graasp",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "prepare": "yarn build",
    "prepack": "yarn build",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\"  --fix",
    "prettier:check": "prettier --check \"{src,test}/**/*.ts\"",
    "prettier:write": "prettier --write \"{src,test}/**/*.ts\"",
    "test": "jest --forceExit --coverage --verbose",
    "test:watch": "yarn test --watchAll",
    "pre-commit": "yarn prettier:check && yarn lint",
    "hooks:uninstall": "husky uninstall",
    "hooks:install": "husky install",
    "post-commit": "git status"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/graasp/graasp-plugin-thumbnails.git"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/graasp/graasp-plugin-thumbnails/issues"
  },
  "homepage": "https://github.com/graasp/graasp-plugin-thumbnails#readme",
  "dependencies": {
    "graasp-plugin-file": "github:graasp/graasp-plugin-file#5/updateDep",
    "graasp-plugin-file-item": "github:graasp/graasp-plugin-file-item#22/updateDep",
    "graasp-plugin-public": "github:graasp/graasp-plugin-public#34/updateDep",
    "sharp": "0.29.3"
  },
  "devDependencies": {
    "@commitlint/cli": "16.0.1",
    "@commitlint/config-conventional": "16.0.0",
    "@types/eslint": "^8.2.2",
    "@types/graasp": "github:graasp/graasp-types",
    "@types/jest": "27.4.0",
    "@types/node": "17.0.8",
    "@types/sharp": "0.29.5",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "eslint": "^8.6.0",
    "fastify": "3.25.3",
    "graasp-test": "github:graasp/graasp-test#13/updateDep",
    "http-status-codes": "2.2.0",
    "husky": "7.0.4",
    "jest": "27.4.7",
    "prettier": "2.5.1",
    "ts-jest": "27.1.2",
    "typescript": "4.4.4",
    "uuid": "8.3.2"
  },
  "packageManager": "yarn@3.2.0-rc.3"
}
