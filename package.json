{
  "name": "graasp-plugin-thumbnails",
  "version": "0.1.0",
  "description": "Thumbnails fastify plugin for Graasp",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "prepare": "yarn build",
    "prepack": "yarn build",
    "lint": "tsc --noEmit && eslint \"{src,test}/**/*.{js,ts}\"  --fix",
    "prettier:check": "prettier --check \"{src,test}/**/*.ts\"",
    "prettier:write": "prettier --write \"{src,test}/**/*.ts\"",
    "test": "jest --forceExit --coverage --verbose",
    "test:watch": "yarn test --watchAll",
    "pre-commit": "yarn prettier:check && yarn lint",
    "hooks:uninstall": "husky uninstall",
    "hooks:install": "husky install",
    "post-commit": "git status"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/graasp/graasp-plugin-thumbnails.git"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/graasp/graasp-plugin-thumbnails/issues"
  },
  "homepage": "https://github.com/graasp/graasp-plugin-thumbnails#readme",
  "dependencies": {
    "@graasp/translations": "github:graasp/graasp-translations",
    "graasp-plugin-file": "github:graasp/graasp-plugin-file#13/redirect",
    "graasp-plugin-file-item": "github:graasp/graasp-plugin-file-item#30/updateDep",
    "graasp-plugin-public": "github:graasp/graasp-plugin-public",
    "sharp": "0.30.3"
  },
  "devDependencies": {
    "@commitlint/cli": "16.2.3",
    "@commitlint/config-conventional": "16.2.1",
    "@types/eslint": "^8.4.1",
    "@types/graasp": "github:graasp/graasp-types",
    "@types/jest": "27.4.1",
    "@types/node": "17.0.23",
    "@types/sharp": "0.30.1",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "eslint": "^8.12.0",
    "fastify": "3.28.0",
    "graasp-test": "github:graasp/graasp-test",
    "http-status-codes": "2.2.0",
    "husky": "7.0.4",
    "jest": "27.5.1",
    "prettier": "2.6.2",
    "ts-jest": "27.1.4",
    "typescript": "4.6.3",
    "uuid": "8.3.2"
  },
  "packageManager": "yarn@3.2.0-rc.3"
}
